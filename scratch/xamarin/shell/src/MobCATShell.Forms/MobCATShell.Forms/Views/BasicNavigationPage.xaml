<?xml version="1.0" encoding="UTF-8"?>
<local:BaseContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace: Microsoft.MobCAT.Forms.Pages"
    xmlns:vm="clr-namespace:MobCATShell.Forms.ViewModels"
    x:Class="MobCATShell.Forms.Views.BasicNavigationPage"   
    x:TypeArguments="vm:BasicNavigationVM">
    <ContentPage.Content>
        <StackLayout
            HorizontalOptions="Center"
            VerticalOptions="Center">
            <Button
                Text="Navigate Page"
                Command="{Binding NavigatePageCommand}" />
            <Button
                Text="Navigate Modal"
                Command="{Binding NavigateModalCommand}" />
        </StackLayout>
    </ContentPage.Content>
</local:BaseContentPage>